{
    "name": "calchen/laravel-aliyun-sms-notification",
    "description": "阿里云短信 Laravel/Lumen 消息通知扩展包（Aliyun SMS message notifications for Laravel/Lumen）",
    "keywords": [
        "laravel",
        "lumen",
        "notification",
        "laravel notification",
        "lumen notification",
        "阿里云",
        "短信服务",
        "国际短信服务",
        "aliyun",
        "sms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "陈恺垣",
            "email": "contact@chenky.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "alibabacloud/sdk": "^1.8",
        "illuminate/notifications": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench": "^3.5|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Calchen\\LaravelAliyunSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calchen\\LaravelAliyunSms\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Calchen\\LaravelAliyunSms\\AliyunSmsNoticeServiceProvider"
            ]
        }
    }
}
